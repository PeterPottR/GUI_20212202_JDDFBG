<Window x:Class="SurviveTheExam.Win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SurviveTheExam"
        mc:Ignorable="d"
        Title="Win" Height="250" Width="400">
    <Grid>
        <Image Source="/win.png" Stretch="Fill"/>
        <Button Click="LB" Width="80" Height="30" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="60,0,0,10" Content="LBOARD" FontSize="15" Background="DarkSlateGray"/>
        <Button Click="MN" Width="80" Height="30" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,120,10" Content="MAIN" FontSize="15"  Background="DarkSlateGray"/>
    </Grid>
</Window>
